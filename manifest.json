{
  "manifest_version": 2,

  "name": "scrape",
  "description": "A Chrome extension that scrapes profile info",
  "version": "1.0",

  "browser_action": {
    "default_icon": "./lib/icon.png",
    "default_popup": "./lib/scrape.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
  "scripts": ["bundle.js"],
  "persistent": true
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["./lib/jquery-3.1.1.min.js"]
  }]
}
